{
  "name": "@project-gamma/react-sdk",
  "version": "1.0.0",
  "description": "React SDK for Project Gamma - Decentralized Prediction Markets",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": [
    "**/*.css",
    "dist/**/*",
    "**/hooks/**/*",
    "**/components/**/*"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/HorizonOracles/Project_Gamma.git",
    "directory": "sdk"
  },
  "homepage": "https://github.com/HorizonOracles/Project_Gamma#readme",
  "bugs": {
    "url": "https://github.com/HorizonOracles/Project_Gamma/issues"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint src --ext .ts,.tsx",
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:watch": "vitest --watch"
  },
  "keywords": [
    "prediction-markets",
    "defi",
    "ethereum",
    "bnb-chain",
    "react",
    "sdk"
  ],
  "author": "Horizon Oracles",
  "license": "MIT",
  "peerDependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "dependencies": {
    "viem": "^2.0.0",
    "wagmi": "^2.0.0",
    "@tanstack/react-query": "^5.0.0"
  },
  "devDependencies": {
    "@testing-library/react": "^14.0.0",
    "@testing-library/jest-dom": "^6.1.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.2.0",
    "@vitest/ui": "^1.0.0",
    "@vitest/coverage-v8": "^1.0.0",
    "eslint": "^8.0.0",
    "jsdom": "^23.0.0",
    "prettier": "^3.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0",
    "vitest": "^1.0.0"
  }
}

